{
  "name": "react-infinite",
  "version": "0.4.1",
  "description": "A browser-ready efficient scrolling container based on UITableView",
  "main": "build/react-infinite.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/seatgeek/react-infinite"
  },
  "keywords": [
    "react",
    "react-component",
    "scrolling",
    "uitableview",
    "infinite"
  ],
  "author": "Gareth Tan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seatgeek/react-infinite/issues"
  },
  "browser": "build/react-infinite.js",
  "scripts": {
    "test": "jest",
    "watch-test": "watch \"npm test\" ./__tests__ ./src",
    "watch-build": "watch \"gulp build -E\" ./examples ./src",
    "build": "gulp build && gulp build -E"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/preprocessor.js",
    "testPathDirs": [
      "<rootDir>/__tests__",
      "<rootDir>/src"
    ],
    "testFileExtensions": [
      "js",
      "jsx"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ],
    "collectCoverage": true
  },
  "homepage": "https://github.com/seatgeek/react-infinite",
  "devDependencies": {
    "browserify": "^9.0.3",
    "coveralls": "^2.11.2",
    "gulp": "^3.8.8",
    "gulp-concat": "^2.4.3",
    "gulp-if": "^1.2.5",
    "gulp-react": "3.0.0",
    "gulp-rename": "^1.2.0",
    "gulp-sourcemaps": "^1.2.4",
    "gulp-uglify": "^1.0.1",
    "jest-cli": "0.4.0",
    "react-tools": "*",
    "reactify": "^1.0.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.0.0",
    "watch": "^0.16.0",
    "yargs": "^1.3.2",
    "react": ">= 0.12.2"
  },
  "dependencies": {
    "lodash.isarray": "^3.0.0",
    "lodash.isfinite": "^3.0.0"
  },
  "peerDependencies": {
    "react": ">= 0.12.2"
  }
}
